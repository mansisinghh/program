#include<stdio.h>
#include<conio.h>
float area( float b, float h);
int main()
{
float a,b,h;
printf("enter the base and height \n");
scanf("%f%f",&b,&h);
a= area(b,h);
printf("the area of triangle is %f \n",a);
return 0;
getch();
}
float area( float b, float h)
{
float ar;
ar=(0.5)*b*h;
return ar;
}
